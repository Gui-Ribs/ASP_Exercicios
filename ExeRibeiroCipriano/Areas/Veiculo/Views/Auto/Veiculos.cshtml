@model Tuple<ExeRibeiroCipriano.Models.Carro, ExeRibeiroCipriano.Models.Moto>


@{
    ViewBag.Title = "Veiculos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main class="centred" style="overflow: hidden">

    <h2>Veículos</h2>
    <img src="~/Imagens/kk.jpeg" width="700px" height="250px"  style="object-fit:contain" />
    <p><b>@Model.Item2.fabricante / @Model.Item2.categoria / @Model.Item2.cilindrada cc / @Model.Item2.transmissao</b></p>
    <img src="~/Imagens/jaguar.jpg"  width="500px" height="250px" style="object-fit:contain" />
    <p><b>@Model.Item1.fabricante / @Model.Item1.tipo / @Model.Item1.nome / @Model.Item1.transmissao</b></p>

</main>



